generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Field {
  id         Int      @id @unique @default(autoincrement())
  fieldName  String   @db.VarChar(255)
  label      String?  @db.VarChar(255)
  selfType   String?  @db.VarChar(255)
  moduleName String?  @db.VarChar(255)
  isPrimary  Boolean? @default(false)
  isUnique   Boolean? @default(false)
  options    Json?
}

model Module {
  id            Int       @id @unique @default(autoincrement())
  createdAt     DateTime? @default(now())
  moduleName    String?   @unique @db.VarChar(255)
  label         String?   @db.VarChar(255)
  category      String?   @db.VarChar(255)
  updateUser    String?   @db.VarChar(255)
  namespaceName String?   @db.VarChar(255)
  meta          Json?
}

model NameSpace {
  id             Int      @id @unique @default(autoincrement())
  createdAt      DateTime @default(now())
  namespacesName String?  @unique @db.VarChar(255)
  label          String?  @db.VarChar(255)
  describe       String?  @db.VarChar(255)
}

model Record {
  id         Int      @id @unique @default(autoincrement())
  createdAt  DateTime @default(now())
  moduleName String?  @db.VarChar(255)
  values     Json?
  createUser String?  @db.VarChar(255)
}

model user {
  id        Int      @id @default(autoincrement()) @db.UnsignedInt
  creatTime DateTime @default(now())
  userName  String?  @unique @db.VarChar(255)
  phone     String?  @unique @db.VarChar(255)
  password  String?  @db.VarChar(255)
}
